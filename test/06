System {
    name = "Atlas";
    float version = "1.0";
    Network {
        interface "eth0" {
            bool dhcp = true;
            string gateway = "192.168.1.1";
        }

        interface "wlan0" {
            bool dhcp = false;
            string gateway = "192.168.1.1";
        }
    }
    Init {
        string[] modules = { "e1000", "iwlwifi" };
        services = { "networking", "ssh", "nginx" };
    }
    Test {
        name = $System.name;
        version = ^version;
        string hello = $.name;
        float THISSHOULDFAIL = ^Kernel.version;
        plswork = $System.Network.interface["eth0"].gateway;
    }
}
